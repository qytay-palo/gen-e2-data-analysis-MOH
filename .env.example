# Environment variables for MOH Data Extraction System
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env file to version control

# =============================================================================
# Primary Database Configuration
# =============================================================================
DB_HOST=localhost
DB_PORT=5432
DB_NAME=polyclinic_db
DB_SCHEMA=public
DB_USER=your_username
DB_PASSWORD=your_password

# =============================================================================
# NEHR Database Configuration
# =============================================================================
NEHR_HOST=nehr-host
NEHR_PORT=5432
NEHR_DB=nehr_database
NEHR_USER=nehr_user
NEHR_PASSWORD=nehr_password

# =============================================================================
# Data Warehouse Configuration (if applicable)
# =============================================================================
DWH_HOST=dwh-host
DWH_PORT=5432
DWH_DB=dwh_database
DWH_USER=dwh_user
DWH_PASSWORD=dwh_password

# =============================================================================
# Notification Configuration
# =============================================================================
# Email
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@domain.com
SMTP_PASSWORD=your-email-password
ALERT_EMAIL=alerts@moh.gov.sg

# Slack (if using)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Microsoft Teams (if using)
TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/YOUR/WEBHOOK/URL
